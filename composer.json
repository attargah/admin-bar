{
    "name": "attargah/admin-bar",
    "description": "This package adds an admin bar for the front pages of websites built with Filament.",
    "keywords": [
        "attargah",
        "laravel",
        "admin-bar"
    ],
    "homepage": "https://github.com/attargah/admin-bar",
    "support": {
        "issues": "https://github.com/attargah/admin-bar/issues",
        "source": "https://github.com/attargah/admin-bar"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "attargah",
            "email": "koguz168@gmail.com",
            "role": "Developer"
        }
    ],

    "require": {
        "php": "^8.1",
      "filament/filament": "^3.0|^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Attargah\\AdminBar\\": "src/",
            "Attargah\\AdminBar\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Attargah\\AdminBar\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attargah\\AdminBar\\AdminBarServiceProvider"
            ],
            "aliases": {
                "AdminBar": "Attargah\\AdminBar\\Facades\\AdminBar"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
